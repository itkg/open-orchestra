{
  "name": "open-orchestra/open-orchestra",
  "license": "MIT",
  "type": "project",
  "description": "Example of an OpenOrchestra Back Office implementation",
  "authors": [
    {
      "name": "OpenOrchestra community",
      "homepage": "https://github.com/open-orchestra/open-orchestra-elastica-bundle/graphs/contributors"
    }
  ],
  "autoload": {
    "psr-0": {
      "": "src/"
    }
  },
  "require": {
    "incenteev/composer-parameter-handler": "~2.0",
    "open-orchestra/open-orchestra-cms-bundle": "1.1.*@dev",
    "open-orchestra/open-orchestra-workflow-function-bundle": "1.1.*@dev",
    "open-orchestra/open-orchestra-media-admin-bundle": "1.1.*@dev",
    "open-orchestra/open-orchestra-model-bundle": "1.1.*@dev",
    "open-orchestra/open-orchestra-base-api-mongo-model-bundle": "1.1.*@dev",
    "open-orchestra/open-orchestra-elastica-bundle": "1.1.*@dev",

    "open-orchestra/open-orchestra-base-bundle": "@dev",
    "open-orchestra/open-orchestra-bbcode-bundle": "@dev",
    "open-orchestra/open-orchestra-libs": "@dev",
    "open-orchestra/open-orchestra-base-api-bundle": "@dev",
    "open-orchestra/open-orchestra-display-bundle": "@dev",
    "open-orchestra/open-orchestra-user-bundle": "@dev",
    "open-orchestra/open-orchestra-model-interface": "@dev",
    "open-orchestra/open-orchestra-media-bundle": "@dev",
    "open-orchestra/open-orchestra-media-file-bundle": "@dev",
    "open-orchestra/open-orchestra-mongo-libs": "@dev",
    "solution/mongo-odm-aggregation-bundle": "@dev",
    "solution/pipeline-builder": "@dev",
    "innocead/captcha-bundle": "@dev",
    "friendsofsymfony/user-bundle": "@dev"
  },
  "require-dev": {
    "jmikola/geojson": "~1.0",
    "phpunit/phpunit" : "4.8.8",
    "phake/phake": "2.1.0",
    "phpdocumentor/reflection-docblock": "2.0.4"
  },
  "scripts": {
    "post-install-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
    ],
    "post-update-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
    ]
  },
  "config": {
    "bin-dir": "bin"
  },
  "extra": {
    "symfony-app-dir": "app",
    "symfony-web-dir": "web",
    "incenteev-parameters": {
      "file": "app/config/parameters.yml"
    }
  }
}
